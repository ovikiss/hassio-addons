{
  "name": "Mediamtx RTSP Simple Server Add-on",
  "url": "https://github.com/ovikiss/mediamtx",
  "version": "0.23.5",
  "slug": "mediamtx",
  "description": "Create RTSP server to broadcast streams, thanks to @aler9 - https://github.com/bluenviron/mediamtx",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
"boot": "auto",
  "legacy": true,
  "ports": {
    "8554/tcp": 8554,
    "1935/tcp": 1935
  },
  "startup": "application",
  "options": {
	  "MTX_PROTOCOLS": "tcp",
	  "MTX_PATHS_ALL_RUNONDEMAND": "echo 'incoming'",
	  "MTX_PATHS_ALL_RUNONDEMANDRESTART": "yes"
  },
  "schema": {
	  "MTX_PROTOCOLS": "list(tcp)",
	  "MTX_PATHS_ALL_RUNONDEMAND": "list(echo 'incoming')",
	  "MTX_PATHS_ALL_RUNONDEMANDRESTART": "list(yes)"
  },
  "image": "bluenviron/mediamtx"
}
